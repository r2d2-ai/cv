{
  "name": "Detector",
  "type": "flogo:app",
  "version": "0.0.1",
  "appModel": "1.20.0",
  "description": "",
  "imports": [
    "github.com/r2d2-ai/contrib/activity/log",
    "github.com/r2d2-ai/cam/trigger/ipcam",
    "github.com/r2d2-ai/flow"
  ],
  "triggers": [
    {
      "id": "ai_box_ipcam",
      "ref": "#ipcam",
      "name": "IP Cam Feed",
      "description": "IP Cam Feed",
      "handlers": [
        {
          "settings": {
            "protocol": "RSTP",
            "host": "192.168.50.195",
            "user": "admin",
            "password": "P$rolaMeaCAM",
            "videoUri": "11"
          },
          "action": {
            "ref": "#flow",
            "settings": {
              "flowURI": "res://flow:detect"
            },
            "input": {
              "fps": "=$.fps"
            }
          }
        }
      ]
    }
  ],
  "resources": [
    {
      "id": "flow:detect",
      "data": {
        "name": "Detect",
        "metadata": {
          "input": [
            {
              "name": "fps",
              "type": "double"
            }
          ]
        },
        "tasks": [
          {
            "id": "log_2",
            "name": "Log",
            "description": "Logs a message",
            "activity": {
              "ref": "#log",
              "input": {
                "message": "=$flow.fps",
                "addDetails": false,
                "usePrint": false
              }
            }
          }
        ]
      }
    }
  ]
}